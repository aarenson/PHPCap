{
	"name" : "phpcap/phpcap",
	"license" : "Apache-2.0",
	"description" : "PHP API for REDCap",
	"keywords" : [
		"REDCap"
	],
	"require" : {
		"php" : ">=5.6.0",
		"ext-curl" : "*",
		"ext-openssl" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "5.7.15",
		"squizlabs/php_codesniffer" : "3.2.3",
		"apigen/apigen" : "v4.1.2",
		"erusev/parsedown" : "1.7.2",
		"kzykhys/php-csv-parser" : "1.4.1"
	},
	"autoload" : {
		"psr-4" : {
			"IU\\PHPCap\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"IU\\PHPCap\\" : [
				"tests/unit",
				"tests/integration"
			]
		},
		"files" : [
			"tests/integration/SystemFunctions.php"
		]
	}
}
